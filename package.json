{
  "name": "cfig-partnersky-program",
  "version": "1.0.0",
  "description": "CFIG Partnersk√Ω program Frontend",
  "main": "index.js",
  "scripts": {
    "start": "browser-sync start --server \"f:/-PROJECTS-/CFIG/partnersky portal/dist\" --files \"dist/**/*.*\" --no-notify --port 3003",
    "copy-js": "cpx \"src/js/**/*.js\" dist/js --watch",
    "copy-jquery": "cpx \"node_modules/jquery/dist/jquery.min.js\" dist/js",
    "copy-images": "cpx \"src/images/**/*.*\" dist/images",
    "scss": "sass src/scss/main.scss dist/css/main.css",
    "scss:watch": "sass --watch src/scss/main.scss:dist/css/main.css",
    "dev": "npm-run-all --parallel copy-jquery copy-images start copy-js scss:watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "concurrently": "^8.2.2",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.85.1"
  },
  "dependencies": {
    "jquery": "^3.7.1"
  }
}
